{
    "version": 2,
    "builds": [
      {
        "src": "backend_api.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend_api.py"
      }
    ],
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  }